    <!-- ======= Header ======= -->
    <header id="header" class="header fixed-top d-flex align-items-center">

        <div class="d-flex align-items-center justify-content-between">
           
            <a href="@Url.Action("Paginainicio", "Usuariologin")" class="logo d-flex align-items-center">
                <img src="~/Scripts/vendor/img/logo.png" alt="">
                <span class="d-none d-lg-block">LyNAdmin</span>
            </a>
            <i class="bi bi-list toggle-sidebar-btn"></i>
        </div><!-- End Logo -->

      

        <nav class="header-nav ms-auto">
            <ul class="d-flex align-items-center">

                @{
                    Html.RenderAction("mostrarnotificaciones", "Usuariologin");
                }

                <li class="nav-item dropdown pe-3">

                    <a class="nav-link nav-profile d-flex align-items-center pe-0" href="#" data-bs-toggle="dropdown">
                        <img src="~/Scripts/vendor/img/profile-img.jpg" alt="Profile" class="rounded-circle">
                        <span class="d-none d-md-block dropdown-toggle ps-2">¡Hola! @Session["Nombreuusuario"].ToString()</span>
                    </a><!-- End Profile Iamge Icon -->

                    <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow profile">
                        <li class="dropdown-header">
                            <h6>Perfil de usuario:</h6>
                            <span>@Session["Perfil"].ToString()</span>
                        </li>
                        <li>
                            <hr class="dropdown-divider">
                        </li>

                        <li>
                            <a class="dropdown-item d-flex align-items-center" href="@Url.Action("Logout", "Usuariologin")">
                                <i class="bi bi-box-arrow-right"></i>
                                <span>Cerrar Sesión</span>
                            </a>
                        </li>

                    </ul><!-- End Profile Dropdown Items -->
                </li><!-- End Profile Nav -->

            </ul>
        </nav><!-- End Icons Navigation -->

    </header><!-- End Header -->
